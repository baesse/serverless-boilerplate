{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "consistent-return": "off",
    "no-console": ["error", { "allow": [ "warn", "error" ]}]
  }
}